{
  "name": "bbc-earth-daily",
  "version": "1.0.0",
  "description": "Posts a BBC Earth video to a Telegram channel",
  "main": "index.js",
  "scripts": {
    "bundle": "browserify index.js --outfile bundle.js",
    "compress": "zip app.zip bundle.js",
    "build": "rm -f bundle.js app.zip && npm run bundle && npm run compress",
    "list": "aws lambda list-functions",
    "info": "aws lambda get-function --function-name bbc-earth-daily",
    "deploy": "aws lambda update-function-code --function-name my-function --zip-file fileb://app.zip --publish",
    "publish": "npm run deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "Jesstern Rays",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.1",
    "lodash": "^4.17.15",
    "query-string": "^6.9.0"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "jest": "^24.9.0",
    "jest-when": "^2.7.0"
  }
}
